{
  "version": 2,
  "builds": [
    { "src": "myportfolio_project/wsgi.py", "use": "@vercel/python" },
    { "src": "staticfiles/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/myportfolio_project/wsgi.py" }
  ]
}
